<!DOCTYPE html>
{% block angularApp %} <html lang="en"> {% endblock %}
<head>
    <title>{% block title %}Welcome to Asphalt Moscow {% endblock %}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="Thu, 19 Nov 1900 08:52:00 GMT">
    <link rel="shortcut icon" href="madmin/images/icons/favicon.ico">
    <link rel="apple-touch-icon" href="madmin/images/icons/favicon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="madmin/images/icons/favicon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="madmin/images/icons/favicon-114x114.png">
    <!--Loading bootstrap css-->
    <link type="text/css" rel="stylesheet"
          href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,700">
    <link type="text/css" rel="stylesheet" href="http://fonts.googleapis.com/css?family=Oswald:400,700,300">
    <link type="text/css" rel="stylesheet"
          href="{{ asset('madmin/vendors/jquery-ui-1.10.4.custom/css/ui-lightness/jquery-ui-1.10.4.custom.min.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/font-awesome/css/font-awesome.min.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/bootstrap/css/bootstrap.min.css') }}">
    <!--LOADING STYLESHEET FOR PAGE--><!--Loading style vendors-->
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/animate.css/animate.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/jquery-lightbox/css/lightbox.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/owl-carousel/owl-carousel/owl.carousel.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/owl-carousel/owl-carousel/owl.theme.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/vendors/jquery-circliful/css/jquery.circliful.css') }}">
    <!--Loading style-->
    <link type="text/css" rel="stylesheet" href="{{ asset('madmin/frontend/assets/css/themes/orange.css') }}">

    {#<link type="text/css" rel="stylesheet" href="{{ asset('bundles/app/css/style.css') }}">#}
    {% block stylesheets %}{% endblock %}
</head>

<body id="glavnoie" data-spy="scroll" data-target=".navbar-custom" class="frontend-one-page dark-layout">
<!--BEGIN PAGE LOADER-->
<div id="page-loader"><img src="http://swlabs.co/images/icon/preloader.gif" alt="" />
</div>
<!--END PAGE LOADER-->
<!--BEGIN BACK TO TOP--><a id="totop" href="#"><i class="fa fa-angle-up"></i></a>
<!--END BACK TO TOP-->
<!--BEGIN CONTENT-->

{% block body %}

{% endblock body %}

{% block footer %}
<footer>
    <div class="container">
        <p class="text-center"><a href="http://my.tuyn.am/">Developed by Rise Technologies Â© 2016</a></p>
    </div>
</footer>
{% endblock footer %}

{% block javascript %}
    <script src="{{ asset('madmin/js/jquery-1.10.2.min.js') }}"></script>
    <script src="{{ asset('madmin/js/jquery-migrate-1.2.1.min.js') }}"></script>
    <script src="{{ asset('madmin/js/jquery-ui.js') }}"></script>
    <!--loading bootstrap js-->
    <script src="{{ asset('madmin/vendors/bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('madmin/vendors/bootstrap-hover-dropdown/bootstrap-hover-dropdown.js') }}"></script>
    <script src="{{ asset('madmin/js/html5shiv.js') }}"></script>
    <script src="{{ asset('madmin/js/respond.min.js') }}"></script>
    <script src="{{ asset('madmin/vendors/isotope/dist/isotope.pkgd.min.js') }}"></script>
    <script src="{{ asset('madmin/vendors/jquery.hoverdir.js') }}"></script>
    <script src="{{ asset('madmin/vendors/modernizr.custom.97074.js') }}"></script>
    <script src="{{ asset('madmin/vendors/jquery-lightbox/js/lightbox.min.js') }}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAu6tm60TzeUo9rWpLnrQ7mrFn4JPMVje4&amp;sensor=false"></script>
    <script src="{{ asset('madmin/vendors/owl-carousel/owl-carousel/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('madmin/vendors/jquery-circliful/js/jquery.circliful.min.js') }}"></script>
    <!--Loading script for each page-->
    <script src="{{ asset('madmin/frontend/assets/js/jquery.appear.js') }}"></script>
    <script src="{{ asset('madmin/frontend/assets/plugins/jquery-wow/wow.min.js') }}"></script>
    <script src="{{ asset('madmin/frontend/assets/plugins/jquery-videoBG/jquery.videoBG.js') }}"></script>
    {#<script src="{{ asset('madmin/frontend/assets/js/one-page_video.js') }}"></script>#}
    <script type="text/javascript">
        $(function() {
            $('.bg-video').videoBG({
                mp4:'madmin/frontend/assets/plugins/jquery-videoBG/assets/tunnel_animation.mp4',
                ogv:'madmin/frontend/assets/plugins/jquery-videoBG/assets/tunnel_animation.ogv',
                webm:'madmin/frontend/assets/plugins/jquery-videoBG/assets/tunnel_animation.webm',
                poster:'madmin/frontend/assets/plugins/jquery-videoBG/assets/tunnel_animation.jpg',
                scale:true
            });
            $('.intro, header').css('height', $(".videoBG_wrapper").height());
        });
    </script>
    <script src="{{ asset('madmin/frontend/assets/js/one-page_animated.js') }}"></script>
    <!--CORE JAVASCRIPT-->
    <script src="{{ asset('madmin/frontend/assets/js/jquery-text-effect.js') }}"></script>
    <script src="{{ asset('madmin/js/frontend-one-page.js') }}"></script>
{% endblock%}
</body>
</html>